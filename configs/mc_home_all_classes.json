{
  "device": "cuda:0",
  "hyper_params": {
    "feature_type": "mels",
    "excerpt_size": 384,
    "batch_size": 16,
    "learning_rate": 1e-5,
    "weight_decay": 1e-6,
    "n_updates": 5e3,
    "overlap_train_excerpts": true,
    "rnd_augment": true
  },
  "network_config": {
    "in_channels": 1,
    "n_kernels": 64,
    "p_dropout": 0.3
  },
  "fft_params": {
    "n_fft": 1024,
    "hop_size": 512,
    "n_mfcc": 64
  },
  "eval_settings": {
    "train_stats_at": 1e2,
    "validate_at": 5e2
  },
  "scenes": [
    "home"
  ],
  "classes": [
    "(object) rustling",
    "(object) snapping",
    "cupboard",
    "cutlery",
    "dishes",
    "drawer",
    "glass jingling",
    "object impact",
    "people walking",
    "washing dishes",
    "water tap running"
  ]
}
